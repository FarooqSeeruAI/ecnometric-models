{
  "scenarios": {
    "1_simple_emiratization": {
      "description": "Simple 15% Emirati employment increase - Quick test",
      "endpoint": "POST /api/v1/scenarios/run",
      "payload": {
        "scenario_name": "emiratization_test_15pct",
        "year": 2023,
        "steps": 1,
        "shocks": {
          "x1labiEmplWgt_EMIRATI": 15.0
        }
      }
    },
    "2_emiratization_with_gdp": {
      "description": "Emirati employment + GDP growth",
      "endpoint": "POST /api/v1/scenarios/run",
      "payload": {
        "scenario_name": "emiratization_gdp_growth",
        "year": 2023,
        "steps": 1,
        "shocks": {
          "x1labiEmplWgt_EMIRATI": 15.0,
          "realgdp": 5.0
        },
        "reporting_vars": [
          "realgdp",
          "employi",
          "x1labioEmplWgt",
          "INCGDP"
        ]
      }
    },
    "3_multi_employment_shocks": {
